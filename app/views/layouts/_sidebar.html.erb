<!-- sidebar starts -->
<div id="sidebar">
      <% if current_user %>
        <h3>Logged in as <%= current_user.name %></h3>
        <p>Click here to <%= link_to "log out.", log_out_path %></p>
      <% else %>
        <h3>Sign In</h3>
        <p>Click here to <%= link_to "log in.", log_in_path %></p>
      <% end %>

	<h3>Sidebar Menu</h3>
	<ul class="sidemenu">				
		<li><%= link_to "Home", root_path %></li>
		<li><%= link_to "Users", users_path %></li>
		<li><%= link_to "Group Notifications", groupnotifications_path %></li>
	</ul>	
		
			
			
				
<!-- sidebar ends -->
</div>